{
  "projectType": "application",
  "generators": {
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "root": "apps/ngxerrors",
  "sourceRoot": "apps/ngxerrors/src",
  "prefix": "snyder",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "options": {
        "outputPath": "dist/apps/ngxerrors",
        "index": "apps/ngxerrors/src/index.html",
        "main": "apps/ngxerrors/src/main.ts",
        "polyfills": "apps/ngxerrors/src/polyfills.ts",
        "tsConfig": "apps/ngxerrors/tsconfig.app.json",
        "aot": true,
        "assets": [
          "apps/ngxerrors/src/favicon.ico",
          "apps/ngxerrors/src/assets"
        ],
        "styles": ["apps/ngxerrors/src/styles.scss"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/ngxerrors/src/environments/environment.ts",
              "with": "apps/ngxerrors/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "buildOptimizer": true,
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2mb",
              "maximumError": "5mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "6kb"
            }
          ]
        }
      },
      "outputs": ["{options.outputPath}"]
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "options": {
        "browserTarget": "ngxerrors:build"
      },
      "configurations": {
        "production": {
          "browserTarget": "ngxerrors:build:production"
        }
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "ngxerrors:build"
      }
    },
    "lint": {
      "executor": "@angular-devkit/build-angular:tslint",
      "options": {
        "tsConfig": [
          "apps/ngxerrors/tsconfig.app.json",
          "apps/ngxerrors/tsconfig.spec.json"
        ],
        "exclude": ["**/node_modules/**", "!apps/ngxerrors/**/*"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "apps/ngxerrors/jest.config.js",
        "tsConfig": "apps/ngxerrors/tsconfig.spec.json",
        "setupFile": "apps/ngxerrors/src/test-setup.ts",
        "passWithNoTests": true
      },
      "outputs": ["coverage/apps/ngxerrors"]
    }
  },
  "tags": []
}
