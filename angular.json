{
  "version": 1,
  "projects": {
    "ngxerrors": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/ngxerrors",
      "sourceRoot": "apps/ngxerrors/src",
      "prefix": "snyder",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/ngxerrors",
            "index": "apps/ngxerrors/src/index.html",
            "main": "apps/ngxerrors/src/main.ts",
            "polyfills": "apps/ngxerrors/src/polyfills.ts",
            "tsConfig": "apps/ngxerrors/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/ngxerrors/src/favicon.ico",
              "apps/ngxerrors/src/assets"
            ],
            "styles": ["apps/ngxerrors/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/ngxerrors/src/environments/environment.ts",
                  "with": "apps/ngxerrors/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            }
          },
          "outputs": ["{options.outputPath}"]
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ngxerrors:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ngxerrors:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ngxerrors:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/ngxerrors/tsconfig.app.json",
              "apps/ngxerrors/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/ngxerrors/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/ngxerrors/jest.config.js",
            "tsConfig": "apps/ngxerrors/tsconfig.spec.json",
            "setupFile": "apps/ngxerrors/src/test-setup.ts",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/ngxerrors"]
        }
      },
      "tags": []
    },
    "ngxerrors-e2e": {
      "root": "apps/ngxerrors-e2e",
      "sourceRoot": "apps/ngxerrors-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ngxerrors-e2e/cypress.json",
            "tsConfig": "apps/ngxerrors-e2e/tsconfig.e2e.json",
            "devServerTarget": "ngxerrors:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ngxerrors:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/ngxerrors-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/ngxerrors-e2e/**/*"]
          }
        }
      },
      "implicitDependencies": ["ngxerrors"],
      "tags": []
    }
  }
}
